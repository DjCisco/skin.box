<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="6666">
	  <defaultcontrol>1</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
	<controls>
	<control type="group">
		<animation type="WindowClose">
			<effect type="fade" time="300" />
			<effect type="slide" end="+1500,0" time="300" />
		</animation>
		<animation type="WindowOpen">
			<effect type="fade" time="300" />
			<effect type="slide" start="+1500,0" time="300" />
		</animation>
		<include>PanelCoordinates</include>
		<control type="panel" id="1">
			<width>1160</width>
			<height>500</height>
			<itemlayout height="50" width="50">
				<control type="image">
					<width>45</width>
					<height>45</height>
					<texture>tile.png</texture>
				</control>
				<control type="image">
					<width>45</width>
					<height>45</height>
					<texture>$INFO[ListItem.Thumb]</texture>
					<bordersize>5</bordersize>
				</control>
			</itemlayout>
			<focusedlayout height="50" width="50">
				<control type="image">
					<width>45</width>
					<height>45</height>
					<texture>button.png</texture>
				</control>
				<control type="image">
					<width>45</width>
					<height>45</height>
					<texture>$INFO[ListItem.Thumb]</texture>
					<bordersize>5</bordersize>
				</control>
			</focusedlayout>
			<content>
				<include>MainMenuIconChooser</include>
			</content>
		</control>
	</control>	
		</controls>
</window>
