<?xml version="1.0" encoding="utf-8"?>
<includes>
    <!-- This defines the default settings of any label -->
	<default type="label">
        <posx>0</posx>
        <posy>0</posy>
        <label></label>
        <height>45</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <selectedcolor>Accent</selectedcolor>
        <disabledcolor>Red</disabledcolor>
	</default>
	<default type="title">
        <posx>0</posx>
        <posy>0</posy>
        <label></label>
     </default>
    <!-- This defines the default settings of a fading label -->
	<default type="fadelabel">
        <posx>0</posx>
        <posy>0</posy>
        <height>80</height>
        <label></label>
        <align>left</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <selectedcolor>Accent</selectedcolor>
        <disabledcolor>Red</disabledcolor>
        <scrollout>false</scrollout>
        <pauseatend>2000</pauseatend>
    </default>
	<!-- This defines the default settings of an Image -->
    <default type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>-</width>
        <height>-</height>
        <align>center</align>
        <aligny>center</aligny>
        <texture>-</texture>
    </default>
	<!-- This defines the default settings of a Button -->
    <default type="button">
        <posx>0</posx>
        <posy>0</posy>
        <width>300</width>
        <height>44</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <label></label>
        <textoffsetx>20</textoffsetx>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <texturefocus border="5">tile.png</texturefocus>
        <texturenofocus border="2">other_textures/buttonnf.png</texturenofocus>
        <pulseonselect>false</pulseonselect>
    </default>
	<!-- This defines the default settings of a Toggle -->
    <default type="togglebutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>300</width>
        <height>44</height>
        <label></label>
        <textoffsetx>20</textoffsetx>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <texturenofocus border="2">other_textures/buttonnf.png</texturenofocus>
        <texturefocus border="5">other_textures/buttonfo.png</texturefocus>
        <alttexturenofocus border="2">other_textures/buttonnf.png</alttexturenofocus>
        <alttexturefocus border="5">other_textures/buttonfo.png</alttexturefocus>
        <align>center</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
	<!-- This defines the default settings of a radio button -->
    <default type="radiobutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>300</width>
        <height>44</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <label></label>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <radiowidth>30</radiowidth>
        <radioheight>30</radioheight>
        <texturenofocus border="2">other_textures/buttonnf.png</texturenofocus>
        <texturefocus border="5">other_textures/buttonfo.png</texturefocus>
        <textureradiofocus>other_textures/radiobuttonfo.png</textureradiofocus>
        <textureradionofocus>other_textures/radiobuttonnf.png</textureradionofocus>
        <textoffsetx>20</textoffsetx>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrolex">
        <posx>0</posx>
        <posy>0</posy>
        <width>300</width>
        <height>44</height>
        <label></label>
        <font>Small</font>
        <spinwidth>26</spinwidth>
        <spinheight>26</spinheight>
        <texturenofocus border="2">other_textures/buttonnf.png</texturenofocus>
        <texturefocus border="5">other_textures/buttonfo.png</texturefocus>
        <textureup>other_textures/arrowupnf.png</textureup>
        <texturedown>other_textures/arrowdownnf.png</texturedown>
        <textureupfocus>other_textures/arrowupfo.png</textureupfocus>
        <texturedownfocus>other_textures/arrowdownfo.png</texturedownfocus>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <posx>0</posx>
        <posy>0</posy>
        <height>15</height>
        <width>15</width>
        <spinwidth>26</spinwidth>
        <spinheight>26</spinheight>
        <textureup>other_textures/arrowupnf.png</textureup>
        <texturedown>other_textures/arrowdownnf.png</texturedown>
        <textureupfocus>other_textures/arrowupfo.png</textureupfocus>
        <texturedownfocus>other_textures/arrowdownfo.png</texturedownfocus>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="sliderex">
        <posx>0</posx>
        <posy>0</posy>
        <height>44</height>
        <width>300</width>
        <texturenofocus border="2">other_textures/buttonnf.png</texturenofocus>
        <texturefocus border="5">other_textures/buttonfo.png</texturefocus>
        <sliderwidth>150</sliderwidth>
        <sliderheight>15</sliderheight>
        <texturesliderbar>other_textures/osd/slider_bg.png</texturesliderbar>
        <textureslidernib border="2">other_textures/osd/slidernibnf.png</textureslidernib>
        <textureslidernibfocus border="2">other_textures/osd/slidernibfo.png</textureslidernibfocus>
        <font>Small</font>
        <textcolor>White</textcolor>
        <focusedcolor>White</focusedcolor>
        <disabledcolor>Red</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <textoffsety>10</textoffsety>
    </default>
    <default type="progress">
        <posx>150</posx>
        <posy>325</posy>
        <texturebg border="6">other_textures/osd/progressback.png</texturebg>
        <lefttexture>-</lefttexture>
        <midtexture border="6">other_textures/osd/progressmid.png</midtexture>
        <righttexture>-</righttexture>
        <overlaytexture>-</overlaytexture>
    </default>
    <default type="slider">
        <posx>0</posx>
        <posy>0</posy>
        <width>420</width>
        <height>15</height>
        <texturefocus>-</texturefocus>
        <texturenofocus>-</texturenofocus>
        <texturesliderbar>other_textures/osd/slider_bg_2.png</texturesliderbar>
        <textureslidernib border="2">other_textures/osd/slidernibnf.png</textureslidernib>
        <textureslidernibfocus border="2">other_textures/osd/slidernibfo.png</textureslidernibfocus>
    </default>
    <default type="scrollbar">
        <width>25</width>
        <texturesliderbackground border="0,14,0,14">other_textures/scrollbarv.png</texturesliderbackground>
        <texturesliderbar border="0,14,0,14">other_textures/scrollbarv_bar_focus.png</texturesliderbar>
        <texturesliderbarfocus border="0,14,0,14">other_textures/scrollbarv_bar_focus.png</texturesliderbarfocus>
        <textureslidernib>other_textures/scrollbarnib.png</textureslidernib>
        <textureslidernibfocus>other_textures/scrollbarnib.png</textureslidernibfocus>
        <showonepage>true</showonepage>
        <orientation>vertical</orientation>
    </default>
    <default type="textbox">
        <label></label>
        <font>Small</font>
        <textcolor>white</textcolor>
        <pagecontrol>61</pagecontrol>
        <autoscroll delay="6000" time="2000" repeat="10000" />
    </default>
</includes>