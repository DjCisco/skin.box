<?xml version="1.0" encoding="utf-8"?>
<window id="115">
    <defaultcontrol always="true">901</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <visible>[VideoPlayer.IsFullscreen | Window.IsVisible(visualisation)] + [Player.Seeking | Player.DisplayAfterSeek | Player.paused | Player.Forwarding | Player.rewinding]</visible>
    <onload>ActivateWindow(fullscreeninfo)</onload>
	<onunload>Dialog.Close(fullscreeninfo)</onunload>
	<controls>
        <control type="group">
           
            <posy>150</posy>
            <visible>!Player.Caching</visible>
            <control type="label">
				
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>$LOCALIZE[112]</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.paused + !Window.IsVisible(10140)</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>FFx2</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.forwarding2x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
