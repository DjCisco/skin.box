<?xml version="1.0" encoding="utf-8"?>
<window id="115">
    <defaultcontrol always="true">901</defaultcontrol>
     
    <visible>[VideoPlayer.IsFullscreen | Window.IsVisible(visualisation)] + [Player.Seeking | Player.DisplayAfterSeek | Player.paused | Player.Forwarding | Player.rewinding]</visible>
	<controls>
        <control type="group">
            <top>150</top>
            <visible>!Player.Caching</visible>
            <control type="label">
				<width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>$LOCALIZE[112]</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.paused + !Window.IsVisible(10140)</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>FFx2</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.forwarding2x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>FFx4</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.forwarding4x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>FFx8</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.forwarding8x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>FFx16</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.forwarding16x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>FFx32</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.forwarding32x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>RWx2</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.rewinding2x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>RWx4</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.rewinding4x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>RWx8</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.rewinding8x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>RWx16</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.rewinding16x</visible>
            </control>
            <control type="label">
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
                <label>RWx32</label>
				<textcolor>18FFFFFF</textcolor>
                <visible>player.rewinding32x</visible>
            </control>
        </control>
        <control type="group">
            
            <top>150</top>
            <visible>!String.IsEmpty(Player.CacheLevel) + Player.Caching</visible>
            <!--
			<control type="image">
                <description>Busy animation</description>
                <left>20</left>
                <top>20</top>
                <width>32</width>
                <height>32</height>
                <texture>other_textures/busy.png</texture>
                <aspectratio>keep</aspectratio>
                <animation effect="rotate" start="360" end="0" center="36,36" time="1200" loop="true" condition="true">conditional</animation>
            </control>
			-->
            <control type="label" id="24">
                <description>buffering label</description>
                <width>1280</width>
                <height>32</height>
                <align>center</align>
                <font>SeekBarState</font>
				<textcolor>18FFFFFF</textcolor>
                <label>$LOCALIZE[15107] $INFO[Player.CacheLevel]%</label>
            </control>
        </control>
				<!-- This Displays the Current Time on the Top Right 		-->
				<control type="label">
					<label>$INFO[System.Time(hh:mm)][COLOR Accent]$INFO[System.Time(xx), ][/COLOR]</label>
					<right>5</right>
					<top>-5</top>
					<height>30</height>
					<width>270</width>
					<font>Clock</font>
					<align>right</align>
					<aligny>top</aligny>
					<shadowcolor>black</shadowcolor>
				</control>
								<!-- This Displays the Current Date on the Top Right -->
				<control type="label">
					<label> $INFO[System.Date(ddd)] • [COLOR Accent]$INFO[System.Date(mmm)] $INFO[System.Date(dd)][/COLOR]</label>
					<left>1000</left>
					<top>30</top>
					<height>100</height>
					<width>270</width>
					<font>Clock_Footer</font>
					<shadowcolor>black</shadowcolor>
					<align>right</align>
					<visible>!Skin.HasSetting(DisplayWeatherInfo)</visible>
				</control>
				<!-- This Displays the Current Weather on the Top Right -->
				<control type="label">
					<!--<label>$INFO[Window(Weather).Property(Current.Temperature)]$INFO[System.TemperatureUnits, ]</label>-->
					<label>$INFO[Weather.Temperature] • $INFO[System.Date(ddd)] • [COLOR Accent]$INFO[System.Date(mmm)] $INFO[System.Date(dd)][/COLOR]</label>
					<left>1000</left>
					<top>30</top>
					<height>100</height>
					<width>270</width>
					<font>Clock_Footer</font>
					<shadowcolor>black</shadowcolor>
					<align>right</align>
					<visible>Weather.IsFetched + Skin.HasSetting(DisplayWeatherInfo)</visible>
				</control>
			<!-- Progress Bar -->
			<control type="group">	
				<animation effect="slide" start="0,70" end="0,0" time="1200" tween="cubic" easing="out">Visible</animation>
				<animation effect="slide" start="0,0" end="0,70" time="100" tween="cubic" easing="in">Hidden</animation>
				<top>685</top>
				<visible>!Window.IsActive(videoosd) + !Window.IsActive(fullscreeninfo)</visible>
				<!-- Background for Progress Bar -->
				<control type="image">
					<description>black bar</description>
					<left>0</left>
					<top>10</top>
					<width>1280</width>
					<height>25</height>
					<fadetime>200</fadetime>
					<texture background="true">tile.png</texture>
				</control>
				<!-- Progress Bar -->
				<control type="progress">
					<description>Progressbar</description>
					<left>110</left>
					<top>20</top>
					<width>1070</width>
					<height>5</height>
					<info>Player.Progress</info>
					<texturebg>progresswhite.png</texturebg>
					<midtexture>progresslime.png</midtexture>
				</control>
				<!-- Player Time Elapsed-->
				<control type="label">
					<left>0</left>
					<top>0</top>
					<width>100</width>
					<label>$INFO[MusicPlayer.Time]</label>
					<align>center</align>
					<font>Panel_Hint</font>
					<textcolor>Accent</textcolor>
				</control>
				<!-- Player Time Remaining-->
				<control type="label">
					<left>1180</left>
					<top>0</top>
					<width>100</width>
					<label>$INFO[MusicPlayer.TimeRemaining,-]</label>
					<align>center</align>
					<font>Panel_Hint</font>
				</control>
			</control>

   </controls>
</window>