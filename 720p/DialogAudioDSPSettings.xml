<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>5</defaultcontrol>
	<controls>
		<control type="group">
			<left>50</left>
			<animation type="WindowClose">
				<effect type="fade" time="300" />
                <effect type="slide" end="+1500,0" time="300" />
            </animation>
            <animation type="WindowOpen">
                <effect type="fade" time="300" />
                <effect type="slide" start="+1500,0" time="300" />
            </animation>
			<!-- Background -->
			<control type="image">
                <top>290</top>
                <width>1160</width>
                <height>360</height>
                <texture>tile.png</texture>
            </control>

			<control type="label" id="2">
				<description>header label</description>
				<top>215</top>
                <height>30</height>
                <width>800</width>
                <font>Panel_Title</font>
                <align>Left</align>
				<aligny>Top</aligny>
                <textcolor>Accent</textcolor>
			</control>
			<control type="grouplist" id="5">
                <description>Control Area</description>
                <top>290</top>
                <width>1160</width>
                <height>360</height>
                <itemgap>0</itemgap>
                <orientation>vertical</orientation>
				<onup>5</onup>
				<ondown>5</ondown>
			</control>
			<control type="image">
                <left>1160</left>
                <top>290</top>
                <width>40</width>
                <height>40</height>
                <texture flipy="true">down_arrow.png</texture>
                <visible>Container(5).HasPrevious</visible>
                <animation effect="fade" time="400">VisibleChange</animation>
            </control>
			<control type="image">
                <left>1160</left>
                <top>620</top>
                <width>40</width>
                <height>40</height>
                <texture>down_arrow.png</texture>
                <visible>Container(5).HasNext</visible>
                <animation effect="fade" time="400">VisibleChange</animation>
            </control>
		</control>
		<control type="button" id="7">
			<description>Default Button</description>
                <align>left</align>
                <focusedcolor>Accent</focusedcolor>
				<font>List_Settings</font>
		</control>
		<control type="radiobutton" id="8">
			<description>Default RadioButton</description>
             <description>Default RadioButton</description>
				<align>left</align>
                <focusedcolor>Accent</focusedcolor>
				<font>List_Settings</font>
		</control>
		<control type="spincontrolex" id="9">
			<description>Default SpinControlex</description>
                <align>left</align>
                <reverse>yes</reverse>
                <focusedcolor>Accent</focusedcolor>
                <textureup>other_textures/arrowupfo.png</textureup>
                <textureupfocus>other_textures/arrowupg.png</textureupfocus>
                <texturedown>other_textures/arrowdownfo.png</texturedown>
                <texturedownfocus>other_textures/arrowdowng.png</texturedownfocus>
				<font>List_Settings</font>
		</control>
		<control type="image" id="11">
			<description>separator image</description>
			<height>2</height>
			<texture></texture>
		</control>
		<control type="edit" id="12">
			<description>Default Edit</description>
			<left>0</left>
			<top>0</top>
			<height>40</height>
			<font>font13</font>
			<textcolor>grey2</textcolor>
			<focusedcolor>white</focusedcolor>
			<texturenofocus border="5">button-nofocus.png</texturenofocus>
			<texturefocus border="5">button-focus2.png</texturefocus>
		</control>
		<control type="sliderex" id="13">
			<description>Default Slider</description>
			<align>left</align>
			<focusedcolor>Accent</focusedcolor>
			<font>List_Settings</font>
		</control>
		<control type="label" id="14">
			<description>Default Label</description>
			<textoffsetx>10</textoffsetx>
			<align>left</align>
			<textcolor>Accent</textcolor>
			<font>Panel_Hint</font>
		</control>

	
	</controls>
</window>
