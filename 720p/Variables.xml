	<variable name="PanelFooterText">    
		<!-- This control displays the unwatched episode at the bottom of the poster panel if unwatched episodes is not 0, when viewing tv shows-->
		<value condition="[!String.IsEqual(ListItem.Property(UnWatchedEpisodes),0)] + Container.Content(tvshows)">[COLOR Accent]$INFO[ListItem.Property(UnWatchedEpisodes),[COLOR White]$LOCALIZE[16101] [/COLOR],][/COLOR]</value>
		<!-- This controls displays the Date that an episode of a tv shows was last played if the nr of unwatched episodes is 0 -->
		<value condition="String.IsEqual(ListItem.Property(UnWatchedEpisodes),0) + !String.IsEmpty(ListItem.LastPlayed) + Container.Content(tvshows)">[COLOR Accent]$INFO[ListItem.LastPlayed,[COLOR White][UPPERCASE]$LOCALIZE[568][/UPPERCASE]: [/COLOR]][/COLOR]</value>
		<!-- This controls displays the tagline of the movie below the Movie Title -->
		<value condition="!String.IsEmpty(ListItem.Tagline) + Container.Content(Movies)">$INFO[ListItem.Tagline]</value>
		<!-- This controls displays the director of a Movie instead of the tagline, if there is no tagline -->
		<value condition="String.IsEmpty(ListItem.Tagline) + Container.Content(Movies) + !String.IsEmpty(ListItem.Director)">[UPPERCASE][COLOR White]$LOCALIZE[20339][/COLOR]  [COLOR Accent]$INFO[ListItem.Director][/COLOR][/UPPERCASE]</value>
		<value condition="Container.Content(Albums) | String.Contains(Container.FolderPath,musicdb://recentlyaddedalbums)  | String.Contains(Container.FolderPath,musicdb://recentlyplayedalbums) | String.Contains(Container.FolderPath,musicdb://singles) | Container.Content(songs) | String.Contains(Container.FolderPath,musicdb://compilations)">$INFO[ListItem.Artist]</value>
		
