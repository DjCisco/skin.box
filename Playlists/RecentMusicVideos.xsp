<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="musicvideos">
    <name>RecentMusicVideos</name>
    <match>all</match>
    <rule field="dateadded" operator="inthelast">
        <value>3650</value>
    </rule>
    <limit>15</limit>
    <order direction="descending">dateadded</order>
</smartplaylist>
