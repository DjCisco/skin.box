<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>RecentEpisodes</name>
    <match>all</match>
    <rule field="dateadded" operator="inthelast">
        <value>365</value>
    </rule>
    <limit>20</limit>
    <order direction="descending">dateadded</order>
</smartplaylist>
