<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="songs">
    <name>LastPlayedSongs</name>
    <match>all</match>
    <rule field="lastplayed" operator="inthelast">
        <value>365</value>
    </rule>
	 <limit>20</limit>
    <order direction="descending">lastplayed</order>
</smartplaylist>
